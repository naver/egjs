// http://jshint.com/docs/options/
// https://github.com/jshint/jshint/blob/master/src/messages.js
// https://gist.github.com/haschek/2595796
{
    "boss": true,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "expr": true,
    "immed": true,
    "noarg": true,
    "onevar": true,
    "es3" : true,
    "quotmark": "double",
    "smarttabs": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "node": true,
    "sub" : true,
    "browser": true,
    "jquery": true,
    "devel": false,
    "globals" : {
        "eg" : true,
        "Hammer" : true,
	"JSON" : true
    }
}
